{
  "name": "redux-spreadsheet",
  "version": "0.1.0",
  "main": "dist/main.cjs.js",
  "module": "dist/main.esm.js",
  "license": "MIT",
  "homepage": "https://github.com/greylemon/redux-spreadsheet#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greylemon/redux-spreadsheet.git"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist coverage dist",
    "build": "yarn build:rollup",
    "build:rollup": "rollup -c",
    "test": "jest",
    "test:staged": "cross-env CI=true node scripts/test.js --env=jsdom --findRelatedTests",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:prettier": "prettier --list-different \"**/*.{css,md,js,jsx,json,ts,tsx}\""
  },
  "files": [
    "dist",
    "src",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/clone-deep": "^4.0.1",
    "@types/draft-js": "^0.10.40",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/redux-mock-store": "^1.0.2",
    "@types/uniqid": "^5.2.0",
    "clone-deep": "^4.0.1",
    "draft-js": "^0.11.6",
    "exceljs": "^4.0.1",
    "fast-memoize": "^2.5.2",
    "react-redux": "^7.2.0",
    "react-sortable-hoc": "^1.11.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "https://github.com/greylemon/react-window",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "undox": "https://github.com/greylemon/undox",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "lint": "^0.7.0",
    "lint-staged": "^10.2.9",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "redux-mock-store": "https://github.com/greylemon/redux-mock-store",
    "resolve": "1.15.0",
    "rollup": "^2.18.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sass-loader": "8.0.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx},!sample": [
      "yarn fix",
      "yarn test:staged"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
