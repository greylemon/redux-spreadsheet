{
  "name": "redux-spreadsheet",
  "version": "0.2.9",
  "main": "dist/main.cjs.js",
  "module": "dist/main.esm.js",
  "license": "MIT",
  "homepage": "https://github.com/greylemon/redux-spreadsheet#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greylemon/redux-spreadsheet.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist coverage dist",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:prettier": "prettier --list-different \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "build:def": "dts-gen -m redux-spreadsheet",
    "start:storybook": "start-storybook -p 4000",
    "build:storybook": "build-storybook -o docs",
    "publish:storybook": "storybook-to-ghpages",
    "prepublish": "yarn build"
  },
  "files": [
    "dist",
    "src",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/clone-deep": "^4.0.1",
    "@types/color": "^3.0.1",
    "@types/draft-js": "^0.10.40",
    "@types/jest": "^24.0.0",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/redux-mock-store": "^1.0.2",
    "@types/uniqid": "^5.2.0",
    "clone-deep": "^4.0.1",
    "color": "^3.1.2",
    "draft-js": "^0.11.6",
    "exceljs": "^4.0.1",
    "fast-formula-parser": "^1.0.6",
    "immer": "^7.0.5",
    "react-contextmenu": "^2.14.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-sortable-hoc": "^1.11.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "https://github.com/greylemon/react-window",
    "redux-thunk": "^2.3.0",
    "undox": "https://github.com/greylemon/undox",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/source-loader": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "3.4.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.9",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "redux-mock-store": "https://github.com/greylemon/redux-mock-store",
    "resolve": "1.15.0",
    "rollup": "^2.18.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sass-loader": "8.0.2",
    "style-loader": "0.23.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "4.42.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn fix",
      "yarn test"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "test/.*\\.test.(ts|tsx|js)$"
  }
}
