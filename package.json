{
  "name": "spreadsheet-redux",
  "version": "0.1.0",
  "main": "dist/main.bundle.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist coverage dist",
    "build": "webpack --display-error-details",
    "test": "jest",
    "test:staged": "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests",
    "eject": "react-scripts eject",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:prettier": "prettier --list-different \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "prepare": "yarn clean && yarn build"
  },
  "files": [
    "dist",
    "src",
    "index.ts"
  ],
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.3.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/clone-deep": "^4.0.1",
    "@types/draft-js": "^0.10.40",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-sortable-hoc": "^0.7.1",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-thunk": "^2.1.0",
    "@types/uniqid": "^5.2.0",
    "clone-deep": "^4.0.1",
    "draft-js": "^0.11.6",
    "events": "^3.1.0",
    "exceljs": "^4.0.1",
    "fast-memoize": "^2.5.2",
    "lint": "^0.7.0",
    "lint-staged": "^10.2.9",
    "node-sass": "^4.14.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-sortable-hoc": "^1.11.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "https://github.com/greylemon/react-window",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typescript": "^3.9.5",
    "undox": "https://github.com/greylemon/undox",
    "uniqid": "^5.2.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "redux-mock-store": "https://github.com/greylemon/redux-mock-store",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.0.6",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn fix",
      "yarn test:staged"
    ]
  }
}
